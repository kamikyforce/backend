{
  "name": "event-management-backend",
  "version": "1.0.0",
  "description": "Backend API for Event Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
