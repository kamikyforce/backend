    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ics": "^3.8.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
